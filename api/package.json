{
  "name": "api",
  "packageManager": "yarn@3.2.3",
  "dependencies": {
    "@pgtyped/cli": "^1.0.2",
    "@pgtyped/query": "^1.0.1",
    "@types/multer": "^1.4.7",
    "amqp-connection-manager": "^4.1.9",
    "amqplib": "^0.10.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "concurrently": "^7.3.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "node-pg-migrate": "^6.2.2",
    "nodemon": "^2.0.19",
    "swagger-ui-express": "^4.5.0",
    "ts-node": "^10.9.1",
    "tsoa": "next"
  },
  "devDependencies": {
    "@types/amqplib": "^0.8.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.14",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-plugin-jest": "^27.0.1",
    "jest": "^29.0.2",
    "pg": "^8.8.0",
    "pgtools": "^0.3.2",
    "supertest": "6.2.3",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.2"
  },
  "scripts": {
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
    "build": "tsoa spec-and-routes && tsc --project tsconfig.build.json",
    "start": "node build/src/server.js",
    "pgt:watch": "pgtyped -w -c pgtyped.config.json",
    "test": "jest -i --forceExit",
    "test:up": "docker compose -f docker-compose.testing.yml up -d",
    "test:down": "docker compose -f docker-compose.testing.yml down",
    "migrate": "node-pg-migrate",
    "testdb:up": "cross-env DATABASE_URL=postgres://user:pass@localhost:5432/test-db node-pg-migrate up",
    "testdb:down": "cross-env DATABASE_URL=postgres://user:pass@localhost:5432/test-db node-pg-migrate down",
    "insert_admin": "node --loader ts-node/esm src/scripts/insert_admin.ts",
    "test:coverage": "jest -i --coverage --forceExit"
  }
}
